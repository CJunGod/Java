    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>Synchronous</servlet-name>
    <servlet-class>com.controller.Synchronous</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>goindex</servlet-name>
    <servlet-class>com.controller.goindex</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>SetState</servlet-name>
    <servlet-class>com.controller.SetState</servlet-class>
  </servlet>









  <servlet-mapping>
    <servlet-name>UserLogin</servlet-name>
    <url-pattern>/servlet/UserLogin</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>UserAdd</servlet-name>
    <url-pattern>/servlet/UserAdd</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>SelectUser</servlet-name>
    <url-pattern>/servlet/SelectUser</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>DelEmail</servlet-name>
    <url-pattern>/servlet/DelEmail</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>EnterEmail</servlet-name>
    <url-pattern>/servlet/EnterEmail</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>updateEmail</servlet-name>
    <url-pattern>/servlet/insertEmail</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Synchronous</servlet-name>
    <url-pattern>/servlet/Synchronous</url-pattern>
  </servlet-mapping>
